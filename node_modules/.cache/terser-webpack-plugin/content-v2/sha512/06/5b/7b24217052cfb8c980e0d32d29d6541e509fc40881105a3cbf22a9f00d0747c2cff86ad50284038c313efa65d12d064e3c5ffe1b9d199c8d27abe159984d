r footer-light ${footerClasses[footerType] || 'footer-static'}`,\r\n          {\r\n            'd-none': footerType === 'hidden'\r\n          }\r\n        )}\r\n      >\r\n        {footer ? (\r\n          footer\r\n        ) : (\r\n          <FooterComponent\r\n            footerType={footerType}\r\n            footerClasses={footerClasses}\r\n          />\r\n        )}\r\n      </footer>\r\n\r\n      {themeConfig.layout.scrollTop === true ? (\r\n        <div className='scroll-to-top'>\r\n          <ScrollToTop showOffset={300} className='scroll-top d-block'>\r\n            <Button className='btn-icon' color='primary'>\r\n              <ArrowUp size={14} />\r\n            </Button>\r\n          </ScrollToTop>\r\n        </div>\r\n      ) : null}\r\n    </div>\r\n  )\r\n}\r\nexport default HorizontalLayout","import { Box, Mail, MessageSquare, CheckSquare, Calendar, FileText, Circle, ShoppingCart, User } from 'react-feather'\n\nexport default [\n  {\n    id: 'apps',\n    title: 'Apps',\n    icon: <Box />,\n    children: [\n      {\n        id: 'todo',\n        title: 'Life Filter',\n        icon: <CheckSquare />,\n        navLink: '/apps/todo'\n      },\n      {\n        id: 'calendar',\n        title: 'Book Ads',\n        icon: <Calendar />,\n        navLink: '/apps/calendar'\n      },\n\n{\n    id: 'soon',\n    title: 'Coming Soon',\n    icon: <Box />,\n    navLink: '#',\n\n    children: [\n{\n        id: 'email',\n        title: 'Tellfari',\n        icon: <Mail />,\n        navLink: '/apps/email'\n      },\n      {\n        id: 'chat',\n        title: 'InstaFriend',\n        icon: <MessageSquare />,\n        navLink: '/apps/chat'\n      }\n\n     ]\n  }\n\n    ]\n  }\n]\n","import { Box, Menu, Info, HelpCircle, Circle, EyeOff, Folder, LifeBuoy, Shield } from 'react-feather'\r\nexport default [\r\n  {\r\n    id: 'misc',\r\n    title: 'Help',\r\n    icon: <HelpCircle />,\r\n    children: [\r\n      {\r\n        id: 'faq',\r\n        title: 'FAQ',\r\n        icon: <He