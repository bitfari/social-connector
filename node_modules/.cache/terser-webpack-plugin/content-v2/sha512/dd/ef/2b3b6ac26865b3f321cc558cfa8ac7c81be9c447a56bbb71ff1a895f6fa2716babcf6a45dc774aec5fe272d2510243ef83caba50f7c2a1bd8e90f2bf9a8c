ings/CreateApikey.js\n// ** Third Party Components\n// ** Reactstrap Imports\n// ** Images\nconst keyOptions=[{value:'full-control',label:'Full Control'},{value:'modify',label:'Modify'},{value:'read-execute',label:'Read & Execute'},{value:'list-folder',label:'List Folder Contents'},{value:'read-only',label:'Read Only'},{value:'read-write',label:'Read Write'}];const CreateApikey_defaultValues={apiKeyName:''};const CreateApiKey=()=>{// ** Hooks\nconst{control,setError,handleSubmit,formState:{errors}}=Object(index_esm[\"e\" /* useForm */])({defaultValues: CreateApikey_defaultValues});const onSubmit=data=>{if(data.apiKeyName.length){return null;}else{setError('apiKeyName',{type:'manual'});}};return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(reactstrap_module[\"l\" /* Card */],{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(reactstrap_module[\"p\" /* CardHeader */],{className:\"pb-0\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(reactstrap_module[\"v\" /* CardTitle */],{tag:\"h4\",children:\"Create an API Key\"})}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(reactstrap_module[\"jb\" /* Row */],{children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(reactstrap_module[\"B\" /* Col */],{md:{size:5,order:0},xs:{size:12,order:1},children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(reactstrap_module[\"m\" /* CardBody */],{children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(reactstrap_module[\"H\" /* Form */],{onSubmit:handleSubmit(onSubmit),children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"mb-2\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(reactstrap_module[\"O\" /* Label */],{className:\"form-label\",children:\"Choose the Api key type you want to create\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(react_select_esm[\"a\" /* default */],{isClearable:false,options:keyOptions,className:\"react-select\",classNamePrefix:\"select\",theme:Utils[\"f\" /* selectThemeColors */],defaultValue:keyOptions[0]})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"]